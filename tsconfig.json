{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "baseUrl": ".",
    "outDir": "",
    "paths": {
      "Src/*": [
        "src/*"
      ]
    }
  },
	// "types": [
	// 	"node",
	// 	"google.analytics",
	// ],
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}

