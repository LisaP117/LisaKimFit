<header>
	<div class="container">
		<div class="header-logo">
			<a href="/" class="header-logo__link" data-logo>
				<svg class="header-logo__icon" width="210" height="90" viewBox="0 0 210 90" aria-describedby="logo">
					<title id="logo">Lisa Kim Fit</title>
					<image xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/images/LKF-GradientAsset.svg" x="0" y="0" width="100%"
						height="66.1"></image>
				</svg>
			</a>
		</div>
		{% include "_partials/components/navigation.njk" %}
		<div class="navigation__mobile navigation__mobile--closed" id="mobilemenu">
			<ul class="navigation__list">
				{% for item in settings.navigation %}
				<li class="navigation__listItem" data-link="{{item.ref}}"><span>{{item.title}}</span></li>
				{% endfor %}
			</ul>
		</div>
	</div>
</header>